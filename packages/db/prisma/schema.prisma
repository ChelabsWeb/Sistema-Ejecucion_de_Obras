// Update the provider if you target another database. PostgreSQL by default.
datasource db {
  provider = "postgresql"
  url      = env("https://eogaiebigvoeiyphwoio.supabase.co")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id          String   @id @default(cuid())
  name        String
  description String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
